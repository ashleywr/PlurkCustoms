{
   "manifest_version": 2,
   "background": {"page": "background.html"},
   "content_scripts": [ {
      "css": [
         "fontello/css/pcustoms-icon-embedded.css",
         "css/emotion_selector.css",
         "css/ui.css",
         "css/onoffswitch.css",
         "css/uploader.css"
      ],
      "js": [
      	"languages.js",

         "js/keycode.js",
         "js/jszip.js",
         "js/jszip-inflate.js",
      	"js/jquery.min.js",
         "js/jquery.wait.js",
      	"js/jquery.livequery.js",
      	"js/jquery.event-filter.min.js",
      	"js/md5.js",
      	"js/jquery.draggable.js",
         "lib/underscore-min.js",
         "lib/nprogress.js",
         "lib/dropzone.js",

         "local/loader.js",

         "extension/plugin.js",

         "extension/lazyLoader.js",
         "extension/adapter.js",
         "extension/gallery.js",
         "extension/emoticonsStorage.js",
      	"extension/main.js",
         "extension/backup.js",
